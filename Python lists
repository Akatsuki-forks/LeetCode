>>>​b=[​1​,​2​,​3​,​4​,​5​]​#alist>>>​a=[​1​,​2​,​3​,​'codingblocks'​,​'mentor'​,​'jatin'​]#alsoalist>>>​a[​0​]​#indexed1>>>​a[​-3​:]​#sliced[​'codingblocks'​,​'mentor'​,​'jatin'​]>>>​a[:][​1​,​2​,​3​,​'codingblocks'​,​'mentor'​,​'jatin'​]>>>​[​5​,​6​,​7​]+a#concatenation[​5​,​6​,​7​,​1​,​2​,​3​,​'codingblocks'​,​'mentor'​,​'jatin'​]>>>​a[​3​]=​64#mutable>>>​print(a)[​1​,​2​,​3​,​64​,​'mentor'​,​'jatin'​]>>>​a.append(​'food'​)#useofappend()>>>​print(a)[​1​,​2​,​3​,​64​,​'mentor'​,​'jatin'​,​'food'​]
